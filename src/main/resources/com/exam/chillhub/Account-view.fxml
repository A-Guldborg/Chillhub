<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: linear-gradient(from 25% 25% to 100% 100%, #734b6d, #42275a);" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.exam.chillhub.controllers.AccountController">
   <children>
      <VBox alignment="CENTER" layoutX="194.0" layoutY="179.0" spacing="8.0" AnchorPane.bottomAnchor="64.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="64.0">
         <children>
            <ImageView fitHeight="150.0" fitWidth="200.0" nodeOrientation="INHERIT" pickOnBounds="true" preserveRatio="true" VBox.vgrow="ALWAYS">
               <image>
                  <Image url="@chillhub-logo.png" />
               </image>
            </ImageView>
            <HBox fx:id="users" alignment="TOP_CENTER" prefHeight="100.0" prefWidth="604.0" spacing="20.0" />
            <Button fx:id="adduserbutton" alignment="CENTER" maxWidth="100.0" mnemonicParsing="false" onAction="#addUserAction" prefWidth="100.0" style="-fx-background-color: gold;" text="Add user" />
            <Button alignment="CENTER" maxWidth="100.0" mnemonicParsing="false" onAction="#deleteUserAction" prefWidth="100.0" text="Delete user" />
         </children>
      </VBox>
      <!--<TextField fx:id="username" layoutX="73.0" layoutY="368.0" visible="false" />-->
